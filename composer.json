{
    "name": "larasell/request-context",
    "description": "Laravel context manager for setting and getting contextual request data easily",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tehran Alishov",
            "email": "tehran.alishov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "LaraSell\\RequestContext\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraSell\\RequestContext\\Providers\\ContextServiceProvider"
            ],
            "aliases": {
                "Context": "LaraSell\\RequestContext\\Facades\\Context"
            }
        }
    }
}